@model RR_hookah.Models.Product



<form method="post">
    <input asp-for="Id" hidden />
    <div class="border p-3">
        <div class="form-group row">
            <h2 class="text-info pl-3">Удалить продукт</h2>
        </div>
        <div class="row">
            <div class="col-8">
                <div class="form-group row">
                    <div class="col-4">
                        <label asp-for="Name" class="form-control"></label>
                    </div>
                    <div class="form-floating col-8">
                        <input asp-for="Name" disabled class="form-control" />
                    </div>
                </div>
                <div class="form-group row">
                    <div class="col-4">
                        <label asp-for="Price" class="form-control"></label>
                    </div>
                    <div class="form-floating col-8">
                        <input asp-for="Price" disabled class="form-control" />
                    </div>
                </div>
                <div class="form-group row">
                    <div class="col-4">
                        <label asp-for="Description" class="form-control"></label>
                    </div>
                    <div class="form-floating col-8">
                        <textarea asp-for="Description" disabled class="form-control summernote"></textarea>
                    </div>
                </div>
                <div class="form-group row">
                    <div class="col-4">
                        <label asp-for="CategoryId" class="form-control"></label>
                    </div>
                    <div class="form-floating col-8">
                        <input asp-for="Category.Name" disabled class="form-control" />
                        <span asp-validation-for="CategoryId" class="text-danger"></span>
                    </div>
                </div>
                <div class="form-group row">
                    <div class="col-8 offset-4 row">
                        <div class="col">
                             <button type="submit" class="btn btn-danger w-100"><i style="margin-right: 10px" class="far fa-edit"></i> Удалить</button>
                        </div>
                        <div class="col">
                            <a asp-action="Index" class="btn btn-info" w-100><i style="margin-right: 10px" class="fas fa-sign-out-alt"></i> Back</a>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-4">
                @if (Model.Id != 0)
                {
                    <img class="img-fluid" src="@WC.ImagePath@Model.Image" width="100%" style="border-radius: 5px; border: 1px solid #bbb" />
                }
                else
                {
                    <img class="img-fluid" src="data:image/jpeg;base64," width="100%" style="border-radius: 5px; border: 1px solid #bbb" />
                }

            </div>
        </div>
    </div>
</form>

@section Scripts{
    @{
        <partial name="_ValidationScriptsPartial" />
    }
    <script>
        document.addEventListener('DOMContentLoaded', () => {
            $('.summernote').summernote({
                height: 200,
                disabled: true,
            });
        })
    </script>
}

